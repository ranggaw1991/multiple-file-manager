{
  "name": "multiple-file-manager",
  "version": "2.0.0",
  "description": "windows application made with electron to modify all of your file in a specified folder",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "pack": "build --dir",
    "release": "build --publish=never --config mfm-build.json",
    "release-linux": "build --publish=never --config mfm-build.json --linux appImage",
    "release-window": "build --publish=never --config mfm-build.json --win nsis, portable"
  },
  "keywords": [
    "file",
    "file management",
    "editor"
  ],
  "author": "Dhany Nurdiansyah",
  "license": "MIT",
  "devDependencies": {
    "electron": "2.0.17",
    "electron-packager": "13.0.1"
  },
  "dependencies": {
    "bootsalert": "^0.1.0",
    "bootstrap": "^4.2.1",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhanyn10/multiple-file-manager.git"
  },
  "bugs": {
    "url": "https://github.com/dhanyn10/multiple-file-manager/issues"
  },
  "homepage": "https://github.com/dhanyn10/multiple-file-manager#readme"
}
